{
  "plugins": [
    "./node_modules/wintersmith-kelvin/",
    "./node_modules/wintersmith-coffee/",
    "./node_modules/wintersmith-browserify/",
    "./plugins/stylesheet_link_tag"
  ],
  "jade": {
    "pretty": false
  },
  "locals": {
    "assets": {
      "css": {
        "all": [
          "/css/*.css"
        ]
      },
      "js": {
        "all": [
         "/js/*.coffee"
        ]
      }
    },
    "mode": "development"
  },
  "require": {
    "json_stringify_once": "./lib/json_stringify_once"
  },
  "ignore": [ ".DS_Store", "*/.DS_Store" ]
}
